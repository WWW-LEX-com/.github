  html.skin-theme-clientpref-night .mw-parser-output section > figure img, html.skin-theme-clientpref-night .mw-parser-output section > div > figure img, html.skin-theme-clientpref-night .mw-parser-output > figure img { background: var(--background-color-disabled-fixed,#C8CCD1); }
}

