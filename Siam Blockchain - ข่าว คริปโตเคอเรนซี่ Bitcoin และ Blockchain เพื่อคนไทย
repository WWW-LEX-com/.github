.coin-marquee { text-size-adjust: none; }

.coin-marquee div { box-sizing: border-box; }

.coin-marquee-wrapper { display: flex; align-items: center; background: rgb(255, 255, 255); border-top: 1px solid rgb(239, 242, 245); border-bottom: 1px solid rgb(239, 242, 245); overflow: hidden; }

.coin-marquee-wrapper--dark { background: rgb(23, 25, 36); border-top: 1px solid rgb(34, 37, 49); border-bottom: 1px solid rgb(34, 37, 49); }

.coin-marquee-wrapper--dark .coin-marquee-header-signature { border-right: 1px solid rgb(64, 66, 78); }

.coin-marquee-wrapper--dark .coin-marquee-header-signature__power-by { color: rgb(128, 138, 157); }

.coin-marquee-wrapper--dark .coin-marquee-item::after { background-color: rgb(64, 66, 78); }

.coin-marquee-wrapper--dark .coin-marquee-item-inner { color: rgb(255, 255, 255); }

.coin-marquee-wrapper--dark .coin-marquee-item-symbol { color: rgb(161, 167, 187); }

.coin-marquee-wrapper--dark .coin-marquee-item .coin-marquee-item--skeleton .coin-marquee-item__icon, .coin-marquee-wrapper--dark .coin-marquee-item .coin-marquee-item--skeleton .coin-marquee-item-info__row { background-color: rgb(50, 53, 70); }

.coin-marquee-wrapper--transparent { background: transparent !important; }

.coin-marquee-header { float: left; width: 141px; }

.coin-marquee-header-signature { overflow: hidden; width: 141px; padding: 12px 16px; box-sizing: border-box; border-right: 1px solid rgb(239, 242, 245); }

.coin-marquee-header-signature svg, .coin-marquee-header-signature div { float: left; }

.coin-marquee-header-signature svg { height: 18px; }

.coin-marquee-header-signature div { height: 17px; display: inline-block; font-weight: 400; font-size: 9px; line-height: 18px; color: rgb(128, 138, 157); }

.coin-marquee-container { display: flex; width: calc(100% - 141px); overflow: hidden; white-space: nowrap; }

.coin-marquee-container__inner { position: relative; display: inline-block; font-size: 0px; line-height: 0; animation-play-state: running; }

.coin-marquee-container__inner:hover { animation-play-state: paused; }

.coin-marquee-item { position: relative; min-width: 220px; display: inline-block; box-sizing: border-box; padding: 0px 16px; }

.coin-marquee-item:hover, .coin-marquee-item:focus { color: inherit; }

.coin-marquee-item::after { content: ""; position: absolute; top: 20px; right: 0px; display: inline-block; background-color: rgb(239, 242, 245); width: 1px; height: 20px; }

.coin-marquee-item-inner { padding: 16px 0px; display: flex; width: 100%; align-items: center; text-decoration: none; color: rgb(0, 0, 0); }

.coin-marquee-item-inner:hover { color: inherit; }

.coin-marquee-item-inner:hover .coin-marquee-item-name { opacity: 0.6; }

.coin-marquee-item__icon { margin-right: 8px; display: inline-block; line-height: 1; min-width: 20px; max-width: 20px; height: 20px; }

.coin-marquee-item__icon img { width: 100%; height: 100%; }

.coin-marquee-item-info { width: 100%; }

.coin-marquee-item-info__row { display: flex; align-items: center; justify-content: space-between; line-height: 1; }

.coin-marquee-item-info__row:last-of-type { margin-top: 2px; }

.coin-marquee-item-name { margin-right: 12px; transition: 0.1s; }

